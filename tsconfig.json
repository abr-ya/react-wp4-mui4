{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "/dist",
    "target": "es5",
    "lib": ["es6", "dom", "es2018.promise", "ES2017", "ES2016.Array.Include"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
